<Application x:Class="WeeklyGrinder.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:wg="clr-namespace:WeeklyGrinder"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <wg:WeekTitleConverter x:Key="WeekTitleConverter"/>
        <wg:VisibleIfError x:Key="VisibleIfError"/>
        <wg:EnabledIfNotFirst x:Key="EnabledIfNotFirst"/>
        <wg:EnabledIfNotLast x:Key="EnabledIfNotLast"/>

        <wg:CellForegroundConverter x:Key="cellForegroundConverter" />
        <wg:CellBackgroundConverter x:Key="cellBackgroundConverter" />
        <wg:CellFontWeightConverter x:Key="cellFontweightConverter" />
    </Application.Resources>
</Application>
